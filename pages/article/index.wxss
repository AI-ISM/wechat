/**index.wxss**/
.feed-item{
  padding: 20rpx;
  margin: 10rpx 0rpx;
  background: #fff;
}
.entry-title{
  padding: 10rpx 0;
  font-weight: bolder;
  font-size: 35rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.statistics-txt{
  display: block;
  font-size: 30rpx;
  color: #666;
  padding: 10rpx 0;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}
.statistics-actions{
  color: #aaa;
  font-size: 30rpx;
}
.item-more{
  float: right
}